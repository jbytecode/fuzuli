(let passed "PASSED!\n")
(let error "ERROR\n")
(let migrated_to_java_test true)

(def i INTEGER)(def a INTEGER)(def toplam INTEGER)
(def h INTEGER)(def j INTEGER)


(let a 100)

# One For. 
(for (let i 0) (< i 10) (inc i)
		(inc a)
)

(if (= a 110) 
	(print passed) 
	(print error)
)



# Two nested for's
(let toplam 0.0)
(for (let i 0) (< i 10) (inc i)
        (for (let j 0) (< j 10) (inc j)
                (inc toplam)
        )
)

(if (= toplam 100)
    (print passed)
    (print error)
)
                




# Three nested for's
(let toplam 0.0)
(for (let i 0) (< i 10) (inc i)
        (for (let j 0) (< j 10) (inc j)
                (for (let h 0) (< h 10) (inc h)
						(inc toplam)
				)
        )
)

(if (= toplam 1000)
    (print passed)
    (print error)
)





# for without block expression
(let i 0)
(for (let a 0) (< a 10) (inc a)
	(inc i)
)

(if (= i 10) (print passed) (print error))

                      
      

