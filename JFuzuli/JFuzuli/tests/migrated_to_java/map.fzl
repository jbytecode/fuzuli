(require "map.nfl")

(let passed "PASSED!\n")
(let error "ERROR\n")

# TEST 1
(let mymap (map_new))
(if (= (typeof mymap) "java.util.HashMap")
	(print passed)
	(print error)
)


# TEST 2
(map_put mymap "name" "Fuzuli")
(map_put mymap "type" "Programming Language")
(print passed)


# TEST 3
(let name (map_get mymap "name"))
(if (= name "Fuzuli")
	(print passed)
	(print error)
)

# TEST 4
(let size (map_size mymap))
(if (= size 2)
	(print passed)
	(print error)
)

# TEST 5
(map_clear mymap)
(let size (map_size mymap))
(if (= size 0)
	(print passed)
	(print error)
)

# TEST 6
(map_put mymap "name" "Fuzuli")
(map_put mymap "type" "Programming Language")
(map_remove mymap "name")
(let size (map_size mymap))
(if (= size 1)
	(print passed)
	(print error)
)

# TEST 7
(map_clear mymap)
(if (= (map_empty mymap) true)
	(print passed)
	(print error)
)


# TEST 8
(map_put mymap "name" "Fuzuli")
(map_put mymap "type" "Programming Language")
(if (= (map_contains_value mymap "Fuzuli") true)
	(print passed)
	(print error)
)

# TEST 9
(if (= (map_contains_key mymap "type") true)
	(print passed)
	(print error)
)

# TEST 10
(if (= (length (map_get_keys mymap)) 2)
	(print passed)
	(print error)
)








