(let passed "PASSED!\n")
(let error "ERROR\n")


(function customsum (params x y)
    (return (+ x y))
)


# TEST 1
(if (= (customsum 4 6) 10)
    (print passed)
    (print error)
)



# TEST 2
(function f (params x)
  (block
    (return (- x 6))
  )
)


(let result1 (- (f 10) (f 11))  )

(if (= result1 -1)
	(print passed)
	(print error)
)



