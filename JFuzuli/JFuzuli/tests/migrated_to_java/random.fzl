(require "random.nfl")

(let passed "PASSED!\n")
(let error "ERROR\n")

# TEST 1
(let a (rndDoubleList 10))
(if (= (length a) 10)
	(print passed)
	(print error)
)

# TEST 2
(let f (nth a 0))
(if (and (<= f 1.0) (>= f 0.0))
	(print passed)
	(print error)
)

# TEST 3
(let b (rndInt))
(if (= (typeof b) "java.lang.Integer")
	(print passed)
	(print error)
)

# TEST 4
(let bool (rndBoolean))
(if (= (typeof bool) "java.lang.Boolean")
	(print passed)
	(print error)
)

# TEST 5
(let bool2 (rndBoolean))
(if (or (= bool2 "true") (= bool2 "false"))
	(print passed)
	(print error)
)

# TEST 6
(let flt (rndFloat))
(if (= (typeof flt) "java.lang.Float")
	(print passed)
	(print error)
)

# TEST 7
(let lng (rndLong))
(if (= (typeof lng) "java.lang.Long")
	(print passed)
	(print error)
)













