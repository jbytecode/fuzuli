(require "/usr/lib/fuzuli/nfl/math.nfl")

(let passed "PASSED!\n")
(let error "ERROR\n")

(let a 3.14)
(let b a)

(if (and (= b 3.14) (= (typeof b) FLOAT))
	(print passed)
	(print error)
)



(let h 3)

(if (and (= h 3) (= (typeof h) INTEGER))
	(print passed)
	(print error)
)

