(require "/usr/lib/fuzuli/nfl/utils.nfl")
(require "/usr/lib/fuzuli/nfl/io.nfl")

(let a (list 3 5 4 3 2 1))
(let sorted1 (sorta a))

(if (and
		(= (nth sorted1 0) 1)
		(= (nth sorted1 1) 2)
		(= (nth sorted1 2) 3)
		(= (nth sorted1 3) 3)
		(= (nth sorted1 4) 4)
		(= (nth sorted1 5) 5)
	)
	(print "PASSED!\n")
	(print "ERROR\n")
)



(let sorted2 (sortd a))

(if (and
        (= (nth sorted2 0) 5)
        (= (nth sorted2 1) 4)
        (= (nth sorted2 2) 3)
        (= (nth sorted2 3) 3)
        (= (nth sorted2 4) 2)
        (= (nth sorted2 5) 1)
    )
    (print "PASSED!\n")
    (print "ERROR\n")
)

