(let toplam 0.0)

(for (let i 0) (< i 10) (let i (+ i 1))
        (for (let j 0) (< j 10) (let j (+ j 1))
                (for (let h 0) (< h 10) (let h (+ h 1))
                		#(print i " " j " " h "\n")
                        (let toplam (+ toplam 1.0))
                        (if (= toplam 100) (break))
                )
                (if (= toplam 100) (break))
        )
        (if (= toplam 100) (break))
)


(if (= toplam 100)
    (print "PASSED!\n")
    (print "ERROR\n")
)

