(require "/usr/lib/fuzuli/nfl/math.nfl")

(let a "3.14")
(let b a)
(type b FLOAT)

(if (and (= b 3.14) (= (typeof b) FLOAT))
	(print "PASSED!\n")
	(print "ERROR\n")
)



(let h PI)
(type h INTEGER)

(if (and (= h 3) (= (typeof h) INTEGER))
	(print "PASSED!\n")
	(print "ERROR\n")
)

